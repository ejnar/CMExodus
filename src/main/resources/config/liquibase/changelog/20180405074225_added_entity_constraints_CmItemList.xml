<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity CmItemList.
    -->
    <changeSet id="20180405074225-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cm_module_id"
                                 baseTableName="cm_item_list"
                                 constraintName="fk_cm_item_list_cm_module_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cm_module"/>

        <addForeignKeyConstraint baseColumnNames="image_id"
                                 baseTableName="cm_item_list"
                                 constraintName="fk_cm_item_list_image_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cm_image"/>

    </changeSet>
</databaseChangeLog>
