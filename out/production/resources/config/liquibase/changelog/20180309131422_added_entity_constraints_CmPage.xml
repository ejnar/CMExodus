<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity CmPage.
    -->
    <changeSet id="20180309131422-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cm_sub_topic_id"
                                 baseTableName="cm_page"
                                 constraintName="fk_cm_page_cm_sub_topic_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cm_sub_topic"/>

        <addForeignKeyConstraint baseColumnNames="cm_pages_id"
                                 baseTableName="cm_page_module"
                                 constraintName="fk_cm_page_module_cm_pages_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cm_page"/>
        <addForeignKeyConstraint baseColumnNames="modules_id"
                                 baseTableName="cm_page_module"
                                 constraintName="fk_cm_page_module_modules_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cm_module"/>
        
    </changeSet>
</databaseChangeLog>
